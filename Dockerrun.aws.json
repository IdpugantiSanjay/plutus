{
  
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "isanjay112/plutus-web:latest",
      "hostname": "client",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "server"
      ],
      "memory": 250
    },
    {
      "name": "server",
      "image": "isanjay112/plutusapi:latest",
      "hostname": "server",
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 80
        }
      ],
      "environment": [
        {
          "name": "PGPASSWORD",
          "value": "sanjay_11"
        },
        {
          "name": "PGUSER",
          "value": "sanjay"
        },
        {
          "name": "PGDATABASE",
          "value": "plutus"
        },
        {
          "name": "PGHOST",
          "value": "plutus-database"
        }
      ],
      "essential": true,
      "memory": 250
    },
    {
      "name": "database",
      "image": "postgres:13",
      "hostname": "plutus-database",
      "portMappings": [
        {
          "hostPort": 5432,
          "containerPort": 5432
        }
      ],
      "environment": [
        {
          "name": "POSTGRES_PASSWORD",
          "value": "sanjay_11"
        },
        {
          "name": "POSTGRES_USER",
          "value": "sanjay"
        },
        {
          "name": "POSTGRES_DB",
          "value": "plutus"
        }
      ],
      "memory": 250
    }
  ]
}